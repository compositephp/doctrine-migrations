{
    "name": "compositephp/doctrine-migrations",
    "description": "Composite DB bridge to Doctrine Migrations",
    "type": "library",
    "authors": [
        {
            "name": "Composite PHP",
            "email": "compositephp@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "compositephp/db": "^0.2.0",
        "doctrine/migrations": "^3.6",
        "symfony/finder": "^6.2",
        "laminas/laminas-file": "^2.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Composite\\DoctrineMigrations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Composite\\DoctrineMigrations\\Tests\\": "tests/"
        }
    }
}
